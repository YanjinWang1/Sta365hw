   "source": [
    "# Question 2:\n",
    "import numpy as np;from scipy import stats; import matplotlib.pyplot as plt\n",
    "n = 3\n",
    "x = stats.norm(loc=0,scale=1).rvs(n)\n",
    "theta_0,tau = 4,1/2\n",
    "parameter_domain = np.linspace(-10,10,1001)\n",
    "likelihoods = stats.norm.logpdf((x[:,np.newaxis])*np.ones(parameter_domain.shape), \n",
    "                            loc=parameter_domain, scale=1).prod(axis=0)\n",
    "prior = stats.norm(loc=theta_0, scale=1/tau**0.5).logpdf(parameter_domain)\n",
    "c,c2 = 500,10 \n",
    "joint = c*likelihoods*prior \n",
    "posterior = stats.norm(loc=(tau*theta_0+x.sum())/(tau+n), scale=1/(tau+n)**0.5).logpdf(parameter_domain)\n",
    "plt.figure(figsize=(16,5))\n",
    "#individual log of likelihood\n",
    "for xi in x:\n",
    "    likelihoods = stats.norm.logpdf(xi, loc=parameter_domain, scale=1)\n",
    "    plt.plot(parameter_domain, c2*likelihoods, label=\"loglikelihood\")\n",
    "plt.plot(parameter_domain, prior, label=\"logprior\")\n",
    "plt.plot(parameter_domain, posterior, label=\"logposterior\")\n",
    "plt.plot(parameter_domain, joint, linestyle=\"--\", label=\"logjoint\")\n",
    "plt.legend(); plt.title(\"Visualize the log of priors, single likelihoods, posteriors and joint distribution\"); "
   ]
  },
